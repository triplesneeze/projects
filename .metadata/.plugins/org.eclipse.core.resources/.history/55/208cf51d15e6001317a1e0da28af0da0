package sudoku;

/**
 *
 * @author Jesse
 */
public class main {
    
    public static void main(String args[]){
        
        int[][] board = new int[9][9];
        
        
        board[0][0] = 8;
        board[0][1] = 4;
        board[0][2] = 2;
        board[1][1] = 5;
        board[2][1] = 1;
        board[2][0] = 3;
        
        board[0][5] = 1;
        board[1][4] = 3;
        board[2][5] = 4;

        board[0][6] = 3;
        board[0][7] = 9;
        board[0][8] = 6;
        board[1][7] = 4;
        board[2][6] = 5;
        board[2][7] = 7;
        
        board[3][1] = 2;
        board[4][1] = 8;
        board[5][1] = 3;
        board[5][2] = 4;
        
        board[3][3] = 4;
        board[4][4] = 2;
        board[4][5] = 3;
        board[5][5] = 6;
        
        board[3][6] = 7;
        board[3][7] = 3;
        board[5][7] = 2;
        
        board[6][1] = 7;
        board[6][2] = 8;
        board[7][1] = 9;
        board[7][2] = 3;
        board[8][0] = 5;
        board[8][1] = 6;
        board[8][2] = 1;
        
        board[6][3] = 1;
        board[8][3] = 3;
        board[8][4] = 4;
        board[8][5] = 9;
        
        board[8][7] = 8;
        board[8][8] = 7;
        board[8][6] = 2;
        board[6][6] = 9;
        board[6][7] = 5;
        board[6][8] = 3;
        
        
        SudokuBoard sb = new SudokuBoard(board);
        
        if(sb.solvable())
            sb.printAnswer();
        
    }
    
}
